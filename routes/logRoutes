const logsController = require('../controllers/logsController')

const express = require('express')

const router = express.Router()

// const logs = require('../models/logs')

// "index" route
router.get('/', logsController.logIndex)

// "new" route
router.get('/new', logsController.logNew)


// "show" route
router.get('/:id', logsController.logShow)

// "create" route
router.post('/', logsController.logCreate)

// "edit" route
router.get('/:id/edit', logsController.logEdit)

// "destroy" route
router.delete('/:id', logsController.logDelete)

// "update" route
router.put('/:id', logsController.logUpdate)


module.exports = router